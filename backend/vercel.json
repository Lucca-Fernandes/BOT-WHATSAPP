{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/(.*)",
      "methods": ["OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "http://localhost:5173",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,x-session-key",
        "Access-Control-Allow-Credentials": "true",
        "Vary": "Origin"
      },
      "status": 200
    },
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "http://localhost:5173",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,x-session-key",
        "Access-Control-Allow-Credentials": "true",
        "Vary": "Origin"
      }
    }
  ]
}